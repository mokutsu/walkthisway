<div class="nested-fields">
  <!-- <button type='button' id='up_reorder'> UP </button>
  <button type='button' id='down_reorder'> DOWN </button> -->

  <i class="fa fa-mouse-pointer" aria-hidden="true"></i>

  <div class ='form_set_waypoint_address'>
    <%= f.label :address,:class=> 'waypoint_label'  %>
    <%= f.text_field :address,:class=>"form_field_create_walk_address"%>
  </div>

  <div class='form_set_waypoint'>
    <%= f.label :description, :class=> 'waypoint_label' %>
    <div>
    <%= f.text_area :description,:class=>"desc_field_create_walk desc_field_waypoint"%></div>
  </div>

  <div>
    <%= f.hidden_field :order, :class=>"order" %>
  </div>

  <%= link_to_remove_association "Remove Waypoint", f, :class=>"cancel_waypoint" %>
  <button class ="plot_btn" type="button">Plot on Map</button>
</div>
