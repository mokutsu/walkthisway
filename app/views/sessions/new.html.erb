<h1>Log In</h1>
<%= form_tag sessions_path, method: :post do %>
  <div>
    <%= label_tag :email %>
    <%= email_field_tag :email %>
  </div>

  <div>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>

  <div>
    <%= label_tag :remember_me %>
    <%= check_box_tag :remember_me %>
  </div>

  <%= submit_tag 'Log In' %>
<% end %>
