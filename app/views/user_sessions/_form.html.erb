<%= form_tag user_sessions_path, :method => :post do %>
  <div class ='form_set'>
    <%= label_tag :email, "Email", :class => 'form_label' %>
    <%= text_field_tag :email %>
  </div>
  <div class = 'form_set'>
    <%= label_tag :password, "Password", :class => 'form_label' %>
    <%= password_field_tag :password %>
  </div>
  <div class="form_set">
    <%= submit_tag "Login" , :class=>'btn_submit' %>
  </div>
<% end %>
