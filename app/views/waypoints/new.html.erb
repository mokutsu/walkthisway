<h1>Waypoints#new</h1>
<p>Find me in app/views/waypoints/new.html.erb</p>

<%= form_for [@walk, @waypoint] do |f| %>

  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
  <div>
    <%= f.label :address %>
    <%= f.text_field :address %>
  </div>
  <div>
    <%= f.submit %>
  </div>

<% end %>
